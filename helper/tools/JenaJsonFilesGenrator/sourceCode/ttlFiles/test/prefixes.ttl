@prefix schema: <http://schema.org/> .
@prefix concept-status: <http://publications.europa.eu/resource/authority/concept-status/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix days: <http://data.sparna.fr/vocabularies/days#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosxl: <http://www.w3.org/2008/05/skos-xl#> .
@prefix euvoc: <http://publications.europa.eu/ontology/euvoc#> .

<http://data.sparna.fr/vocabularies/days> a skos:ConceptScheme ;
	dct:description "The days of the week"@en ;
	dct:title "Weekdays"@en ;
	skos:hasTopConcept days:friday , days:monday , days:thursday , days:tuesday , days:wednesday , days:week-end .

days:friday a skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:definition "Week-end soon√Ç !"@en ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "FRI" ;
	skos:prefLabel "Friday"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .

days:monday a skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:definition "The day where you get back at work"@en ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "MON" ;
	skos:prefLabel "Monday"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .

days:saturday a skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:broader days:week-end ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "SAT" ;
	skos:prefLabel "Saturday"@en .

days:sunday a skos:Concept ;
	euvoc:comment "Note that some consider Sunday to be the first day of the week. Should we give ordering numbers to weekdays ?"@en ;
	euvoc:status concept-status:CURRENT ;
	skos:broader days:week-end ;
	skos:definition "The day you sleep."@en ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "SUN" ;
	skos:prefLabel "Sunday"@en .

days:thursday a owl:Class, skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "THU" ;
	skos:prefLabel "Thursday"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .

days:tuesday a skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:definition "The most productive day of the week."@en ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "TUE" ;
	skos:prefLabel "Tuesday"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .

days:wednesday a skos:Concept ;
	euvoc:status concept-status:CURRENT ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:notation "WED" ;
	skos:prefLabel "Wednesday"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .

days:week-end a skos:Concept ;
	euvoc:comment "Check with Andrew if the concept of Week-end is the same in the US, in China and the UK. Try to determine a universal notion of what a \"week-end\" is."@en ;
	euvoc:status concept-status:TESTING ;
	skos:definition "Any day of the week-end"@en ;
	skos:inScheme <http://data.sparna.fr/vocabularies/days> ;
	skos:narrower days:saturday , days:sunday ;
	skos:notation "WKND" ;
	skos:prefLabel "Week-end"@en ;
	skos:topConceptOf <http://data.sparna.fr/vocabularies/days> .
